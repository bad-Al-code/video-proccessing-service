{
  "name": "video-processing-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "format": "npx prettier . --write",
    "format:check": "prettier '**/*.ts' --check",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "test:coverage": "npx vitest --coverage",
    "test:ui": "npx vitest --ui",
    "test:ci": "npx vitest --run",
    "test": "npx vitest",
    "lint": "npx eslint .",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "start": "nodemon",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "amqplib": "^0.10.7",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "fluent-ffmpeg": "^2.1.3",
    "mysql2": "^3.14.0",
    "pino": "^9.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/amqplib": "^0.10.7",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.13.14",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.23.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^3.0.9"
  }
}
